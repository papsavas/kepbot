{
	"$schema": "https://turbo.build/schema.json",
	"envMode": "loose",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/", "!.next/cache/"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"start": {
			"persistent": true,
			"cache": false
		}
	}
}
